{
  "extends": [
    "@commitlint/config-angular"
  ],
  "rules": {
    "type-enum": [
      2,
      "always",
      [
        "chore",
        "docs",
        "feat",
        "fix",
        "perf",
        "refactor",
        "revert",
        "style",
        "test",
        "types",
        "build",
        "ci",
        "typings"
      ]
    ],
    "scope-case": [
      0
    ]
  }
}
